{
  "expo": {
    "name": "react_native_yarn",
    "slug": "react_native_yarn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.re1nger.react_native_yarn"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "test-scheme",
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
